{
  "builds": [{
    "src": "app.py",
    "use": "@vercel/python",
    "config": {
      "runtime": "python3.9",
      "installCommand": "pip install fbprophet-0.7.1-cp39-cp39-manylinux2010_x86_64.whl && pip install -r requirements.txt"
    }
  }],
  "routes": [{
    "src": "/(.*)",
    "dest": "app.py"
  }]
}
