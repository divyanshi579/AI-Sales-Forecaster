{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "installCommand": "pip install https://prophet-wheels.s3.amazonaws.com/fbprophet-0.7.1-cp310-cp310-manylinux_2_17_x86_64.whl && pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
